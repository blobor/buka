    "production": [
      "> 1%",
      "last 2 versions",
      "Firefox ESR",
      "not ie 10",
      "not ie_mob 10"
    ],
    "development": [
      "last 1 version",
      "not ie > 0",
      "not ie_mob > 0"
    ]
  },
  "jest": {

  "scripts": {
    "prebuild": "rm -rf public && rm -rf dist-server && mkdir public && mkdir dist-server",
    "build": "concurrently --raw \"npm run build:server\" \"npm run build:client\"",
    "build:server": "gulp build:server",
    "build:client": "webpack --config config/webpack.config.client.js --color --progress",
    "start:watch": "gulp start:watch",
    "lint": "standard --verbose | snazzy",
    "test": "jest",
    "test:ci": "npm run lint && npm run test:coverage",
    "test:coverage": "npm run test -- --coverage",
    "build:client:watch": "webpack --config config/webpack.config.dev.js --color --watch --progress"
  },
